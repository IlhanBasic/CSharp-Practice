<Window x:Class="vojska.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:vojska"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Menu Grid.Row="0">
            <MenuItem Header="Dodaj vojnika" Click="MenuItem_Click"/>
            <MenuItem Header="Lista vojnika" Click="MenuItem_Click_1"/>
        </Menu>
        <Frame x:Name="mainContent" NavigationUIVisibility="Hidden" Grid.Row="1"/>
    </Grid>
</Window>
