<Window x:Class="zadatakNarandze.prikazNarandzi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:zadatakNarandze"
        mc:Ignorable="d"
        Title="prikazNarandzi" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Menu>
            <MenuItem Header="Gajbica" Click="MenuItem_Click"/>
            <MenuItem Header="Baza" Click="MenuItem_Click_1"/>
        </Menu>
        <Frame Grid.Row="1" x:Name="podloga" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
