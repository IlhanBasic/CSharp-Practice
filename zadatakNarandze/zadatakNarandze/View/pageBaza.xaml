<Page x:Class="zadatakNarandze.View.pageBaza"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:zadatakNarandze.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      DataContext="{Binding RelativeSource={RelativeSource Self}}"
      Title="pageBaza">

    <Grid>
        <DataGrid ItemsSource="{Binding gajba}">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Datum Kreiranja Gajbice" Width="*"/>
                <DataGridTextColumn Header="Broj Vrsta" Width="*"/>
                <DataGridTextColumn Header="Broj Kolona" Width="*"/>
                <DataGridTextColumn Header="Broj pokvarenih narandzi" Width="*"/>
                <DataGridTextColumn Header="Broj zdravih narandzi" Width="*"/>
                <DataGridTextColumn Header="Broj Procenat" Width="*"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
